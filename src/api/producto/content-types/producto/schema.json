{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "identifica": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "precio_base": {
      "type": "decimal",
      "required": true
    },
    "tienda": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tienda.tienda",
      "inversedBy": "productos"
    },
    "producto_impuestos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto-impuesto.producto-impuesto",
      "mappedBy": "producto"
    },
    "inventarios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventario.inventario",
      "mappedBy": "producto"
    }
  }
}
